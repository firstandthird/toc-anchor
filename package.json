{
  "name": "toc-anchor",
  "version": "2.0.0",
  "description": "Automatically adds anchors to elements",
  "main": "index.js",
  "module": "dist/toc-anchor.esm.js",
  "scripts": {
    "start": "scriptkit",
    "build": "scriptkit build",
    "dev": "scriptkit dev",
    "pretest": "scriptkit build",
    "test": "cat test/toc-anchor.test.dist.js | tape-run --static . --browser phantomjs --render tap-spec",
    "prepublishOnly": "scriptkit"
  },
  "keywords": [
    "browser",
    "toc",
    "anchor"
  ],
  "author": "First+Third <code@firstandthird.com> (http://firstandthird.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstandthird/toc-anchor.git"
  },
  "bugs": {
    "url": "https://github.com/firstandthird/toc-anchor/issues"
  },
  "homepage": "https://github.com/firstandthird/toc-anchor#readme",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-firstandthird": "4.3.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-import": "^2.16.0",
    "phantomjs-prebuilt": "2.1.16",
    "scriptkit": "2.0.3",
    "tap-spec": "5.0.0",
    "tape-rollup": "4.6.4",
    "tape-run": "5.0.0"
  },
  "scriptkit": {
    "serve": {
      "enabled": true
    },
    "files": {
      "dist/toc-anchor.js": "index.js",
      "test/toc-anchor.test.dist.js": "test/toc-anchor.test.js"
    },
    "scripts": {
      "babel": {
        "exclude": [
          "node_modules/tape-rollup/*"
        ]
      }
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "firstandthird"
  },
  "dependencies": {
    "domassist": "^2.0.2"
  }
}
